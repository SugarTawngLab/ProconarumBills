{
  "_version": "1.65.0",
  "sap.app": {
    "id": "ns.proconarumbills",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.3",
      "toolsId": "8e1a2b61-e383-4164-876a-fbe39d4113d9"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/CustomerService/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.134.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ns.proconarumbills.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "requests": {
        "dataSource": "mainService",
        "settings": {
          "operationMode": "Server"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "ns.proconarumbills.view",
        "async": true,
        "viewPath": "ns.proconarumbills.view"
      },
      "routes": [
        {
          "name": "ListRequest",
          "pattern": ":?query:",
          "target": [
            "ListRequest"
          ]
        },
        {
          "pattern": "CreateRequest",
          "name": "CreateRequest",
          "target": "CreateRequest"
        },
        {
          "pattern": "RequestDetail/{RequestId}",
          "name": "RequestDetail",
          "target": "RequestDetail"
        }        
      ],
      "targets": {
        "ListRequest": {
          "id": "ListRequest",
          "viewId": "ListRequest",
          "viewName": "ListRequest"
        },
        "CreateRequest": {
          "id": "CreateRequest",
          "viewName": "CreateRequest",
          "viewId": "CreateRequest"
        },
        "RequestDetail": {
          "id": "RequestDetail",
          "viewName": "RequestDetail",
          "viewId": "RequestDetail"
        }
      }
    },
    "rootView": {
      "viewName": "ns.proconarumbills.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
